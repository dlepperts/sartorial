<h1>Tailors</h1>
      
<h2>Featured Tailor: <%= link_to @tailor.name, tailor_path(@tailor)%></h2>
    <ul>
    <% @tailors.each do |tailor| %>
        <h3><li><%= link_to tailor.name, tailor_path(tailor) %></h3>
        <p>Average Rating: <%= tailor.display_ratings %></p>
        
        <p><%= link_to tailor.review, tailor_path(tailor) %></p>
        
    <% end %>
    </ul>

<%= link_to 'New Tailor', new_tailor_path %>
